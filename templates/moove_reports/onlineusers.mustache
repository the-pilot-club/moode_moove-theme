<div class="card card-body">
    <form action="{{config.wwwroot}}/theme/moove/reports/onlineusers.php" method="get" class="form">
        <div class="form-row">
            <div class="form-group col-md-5">
                <label for="starttime">{{#str}}starttime, theme_moove{{/str}}</label>
                <input type="datetime-local" class="form-control" id="starttime" name="starttime" value="{{{starttime}}}" required>
            </div>
            <div class="form-group col-md-5">
                <label for="endtime">{{#str}}endtime, theme_moove{{/str}}</label>
                <input type="datetime-local" class="form-control" id="endtime" name="endtime" value="{{{endtime}}}" required>
            </div>
            <div class="form-group col-md-2">
                <label>&nbsp</label>
                <button type="submit" class="btn btn-primary btn-block">{{#str}}filter, theme_moove{{/str}}</button>
            </div>
        </div>
    </form>
</div>

<h2 class="my-3">{{#str}}siteactivity, theme_moove{{/str}}</h2>

<div class="card card-body mt-3">
    {{#error}}
        <div class="alert alert-danger" role="alert">
            <h4 class="alert-heading">{{#str}}onlineusers_error, theme_moove{{/str}}</h4>
            <p>{{#str}}onlineusers_error_desc, theme_moove{{/str}}</p>
        </div>
    {{/error}}

    {{{ chart }}}
</div>